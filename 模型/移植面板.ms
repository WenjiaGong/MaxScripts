rollout PBmixerroll "SelectionMixer"
(
		button but_se1  "Sel"  width:25 Offset:[-10,0] across:3
		button but_shouji1  "Save_____A"  width:80 Offset:[5,0]
		checkbutton chck_1 ""	width:10 height:20 Offset:[20,0] highlightcolor:(color 0 255 0)  enabled:false
		button but_se2  "Sel"  width:25 Offset:[-10,0] across:3	
		button but_shouji2  "Save_____B"  width:80 Offset:[5,0]
		checkbutton chck_2 ""	width:10 height:20 Offset:[20,0] highlightcolor:(color 0 255 0)  enabled:false
		button but_a_b "A-B"  width:23	Offset:[0,0] across:5
		button but_c   "-C-"  width:23	Offset:[2,0]
		button but_b_a "B-A"  width:23	Offset:[6,0]
		button but_ab  "A+B"  width:23 Offset:[11,0]
		button but_o 	"end"  width:23	Offset:[12,0]
	
		local vert_1,vert_2,edge_1,edge_2,face_1,face_2 
		local vert_1=vert_2=edge_1=edge_2=face_1=face_2 =#()
		on but_shouji1 pressed  do ( 
			if selection.count == 1 and (classof $) == Editable_Poly then (
				if subobjectlevel == 1 then(
				 vert_1 = (polyop.getVertSelection  $) as array ; chck_1.checked = true	
				)else(
					if subobjectlevel == 2 then(
						edge_1 = (polyop.getEdgeSelection $) as array ; chck_1.checked = true
					)else(
						if subobjectlevel == 4 or subobjectlevel == 5 then (
							face_1 = (polyop.getfaceSelection $) as array ; chck_1.checked = true	
						)
					)
				)
			)
		)
		on but_shouji2 pressed  do(
			if selection.count == 1 and (classof $) == Editable_Poly then (
				if subobjectlevel == 1 then(
				 vert_2 = (polyop.getVertSelection  $) as array ; chck_2.checked = true	
				)else(
					if subobjectlevel == 2 then(
						edge_2 = (polyop.getEdgeSelection $) as array ; chck_2.checked = true
					)else(
						if subobjectlevel == 4 or subobjectlevel == 5 then (
							face_2 = (polyop.getfaceSelection $) as array ; chck_2.checked = true	
						)
					)
				)
			)
		)
		on but_se1 pressed  do(
			if selection.count == 1 and (classof $) == Editable_Poly then (
				if subobjectlevel == 1 then(
				 polyop.setVertSelection $ vert_1 ; update $
				)else(
					if subobjectlevel == 2 then(
						polyop.setedgeselection $ edge_1 ; update $
					)else(
						if subobjectlevel == 4 or subobjectlevel == 5 then (
							polyop.setfaceselection $ face_1 ; update $
						)
					)
				)
			)
		)
		on but_se2 pressed  do(
			if selection.count == 1 and (classof $) == Editable_Poly then (
				if subobjectlevel == 1 then(
				 polyop.setVertSelection $ vert_2 ; update $
				)else(
					if subobjectlevel == 2 then(
						polyop.setedgeselection $ edge_2 ; update $
					)else(
						if subobjectlevel == 4 or subobjectlevel == 5 then (
							polyop.setfaceselection $ face_2 ; update $
						)
					)
				)
			)
		)
		on but_a_b pressed  do(
			if selection.count == 1 and (classof $) == Editable_Poly then (
				if subobjectlevel == 1 then(
				 polyop.setVertSelection $ (for o in vert_1 where finditem vert_2 o==0 collect o) ; update $
				)else(
					if subobjectlevel == 2 then(
						polyop.setedgeselection $ (for o in edge_1 where finditem edge_2 o==0 collect o) ; update $
					)else(
						if subobjectlevel == 4 or subobjectlevel == 5 then (
							polyop.setfaceselection $ (for o in face_1 where finditem face_2 o==0 collect o) ; update $
						)
					)
				)
			)
		)
		on but_c pressed  do(
			if selection.count == 1 and (classof $) == Editable_Poly then (
				if subobjectlevel == 1 then(
				 polyop.setVertSelection $ (for o in vert_1 where finditem vert_2 o!=0 collect o)  ; update $
				)else(
					if subobjectlevel == 2 then(
						polyop.setedgeselection $ (for o in edge_1 where finditem edge_2 o!=0 collect o) ; update $
					)else(
						if subobjectlevel == 4 or subobjectlevel == 5 then (
							polyop.setfaceselection $ (for o in face_1 where finditem face_2 o!=0 collect o) ; update $
						)
					)
				)
			)
		)
		on but_b_a pressed  do(
			if selection.count == 1 and (classof $) == Editable_Poly then (
				if subobjectlevel == 1 then(
				 polyop.setVertSelection $ (for o in vert_2 where finditem vert_1 o==0 collect o)  ; update $
				)else(
					if subobjectlevel == 2 then(
						polyop.setedgeselection $ (for o in edge_2 where finditem edge_1 o==0 collect o) ; update $
					)else(
						if subobjectlevel == 4 or subobjectlevel == 5 then (
							polyop.setfaceselection $ (for o in face_2 where finditem face_1 o==0 collect o) ; update $
						)
					)
				)
			)
		)
		on but_ab pressed  do(
			if selection.count == 1 and (classof $) == Editable_Poly then (
				if subobjectlevel == 1 then(
				 polyop.setVertSelection $ (vert_1+vert_2)  ; update $
				)else(
					if subobjectlevel == 2 then(
						polyop.setedgeselection $ (edge_1+edge_2) ; update $
					)else(
						if subobjectlevel == 4 or subobjectlevel == 5 then (
							polyop.setfaceselection $ (face_1+face_2) ; update $
						)
					)
				)
			)
		)
		on but_o pressed  do (
			chck_1.checked = chck_2.checked = false	
			vert_1 = vert_2 = edge_1 = edge_2 = face_1 = face_2 = #()	
		)
)

