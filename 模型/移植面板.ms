fn PolyedgeDivider val = (
	if selection.count == 1 then(
		obj = selection[1]
		if classof (modPanel.getCurrentObject ()) == Editable_Poly then (
			vert_1 = #{1..(polyop.getNumVerts obj)}
			selEdges = (getedgeselection obj) as array
			for e in selEdges do(
				setedgeselection obj #(e)
				for num = 0 to (val-2) do(
					if num != 0 then myEdge = #(polyop.getNumEdges obj)
					else myEdge = (getedgeselection obj) as array
					for i in myEdge do(
						polyop.divideEdge obj i (1/((val - num) as float))
					)
				)
			)
			vert_2 = #{1..(polyop.getNumVerts obj)}
			polyop.setVertSelection obj (for o in vert_2 where finditem vert_1 o==0 collect o)  ; update obj
			subobjectLevel = 1
		)
	)
)	
