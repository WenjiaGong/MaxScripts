	checkbutton PBrollbut2 "M" width:23 height:20 highlightcolor: (color 158 234 180) checked:PBo_Tabstates[2]   images:#(load_bitmap_Main_but(),load_bitmap_Main_butM(),4,2,2,1,1) offset:[1,0] tooltip: "Modeling"
	checkbutton PBrollbut3 "P" width:23 height:20 highlightcolor: (color 158 234 180) checked:PBo_Tabstates[3]   images:#(load_bitmap_Main_but(),load_bitmap_Main_butM(),4,3,3,3,3) offset:[5,0] tooltip: "Patternmaker"
	checkbutton PBrollbut4 "Sm" width:23 height:20 highlightcolor: (color 158 234 180) checked:PBo_Tabstates[4]   images:#(load_bitmap_Main_but(),load_bitmap_Main_butM(), 4,4,4,4,4) offset:[10,0] tooltip: "SelectionMixer"
