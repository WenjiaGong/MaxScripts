try(destroydialog PBo_mainfloat)catch()
global PBo_mainfloat,PBselroll , PBmodelroll , PBpatternroll , PBmixerroll , PBo_screensize , PBo_interface , PBo_Tabstates , PBo_rollorder
if PBo_screensize == undefined do PBo_screensize = gw.getWinSizeY()
if PBo_interface == undefined do PBo_interface = #([127,470],[50,205])
if PBo_Tabstates == undefined do PBo_Tabstates = #(true,true,false,false,454)
if PBo_rollorder == undefined do PBo_rollorder = #(1,2)
	
fn RollAddRem leroll add rollcheck =(
	try(
		PBo_mainfloat.height = PBo_mainfloat.height
	)catch()
	
	if keyboard.controlpressed then (
		allbutts = #(PBo_mainfloat.PBrollbut,PBo_mainfloat.PBrollbut2,PBo_mainfloat.PBrollbut3,PBo_mainfloat.PBrollbut4)
		for f = 1 to 4 do(
			if f == rollcheck then allbutts[f].state = true else allbutts[f].state = false
		)
		PBo_rollorder = #(rollcheck)
		nurolls = PBo_mainfloat.PBmainsub.rollouts
		for g in nurolls do (removeSubRollout PBo_mainfloat.PBmainsub g)
		rollarr = #(PBselroll, PBmodelroll, PBpatternroll, PBmixerroll)
		addSubRollout PBo_mainfloat.PBmainsub rollarr[rollcheck]
	)else(
		finroll = finditem PBo_rollorder rollcheck
		if finroll == 0 then append PBo_rollorder rollcheck else deleteitem PBo_rollorder finroll
		if add then addSubRollout PBo_mainfloat.PBmainsub leroll
			else removeSubRollout PBo_mainfloat.PBmainsub leroll
	)
	
	currentrolls = PBo_mainfloat.PBmainsub.rollouts
	allrollhigh = 45
	
	for i in currentrolls do allrollhigh += (i.height + 24)
	curwin = PBo_screensize - 26
	
	if allrollhigh > curwin do allrollhigh = curwin
		if PBo_Dockstate != undefined then (PBo_interface[1])[2] = allrollhigh
			else PBo_mainfloat.height = allrollhigh
	PBo_mainfloat.lb1.pos.y = allrollhigh - 16
)
try(destroydialog PBo_mainfloat)catch()
